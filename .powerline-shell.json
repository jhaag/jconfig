{
  "segments": [
    "virtual_env",
    "uptime",
    "time",
    "username",
    "ssh",
    "cwd",
    "git",
    "git_stash",
    "jobs",
    "set_term_title",
    "newline",
    "rbenv",
    "root",
    "exit_code"
